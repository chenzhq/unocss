{
  "name": "@unocss/vscode",
  "displayName": "UnoCSS",
  "version": "0.30.6",
  "private": true,
  "description": "UnoCSS for VS Code",
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unocss/unocss",
    "directory": "packages/vscode"
  },
  "publisher": "antfu",
  "main": "./dist/index.js",
  "preview": true,
  "scripts": {
    "build": "tsup",
    "dev": "nr build --watch src",
    "publish": "esno ./scripts/publish.ts"
  },
  "contributes": {
    "commands": [],
    "configuration": {
      "type": "object",
      "title": "UnoCSS",
      "properties": {
        "unocss.root": {
          "type": "string",
          "description": "Project root that contains the UnoCSS configuration file"
        }
      }
    }
  },
  "activationEvents": [
    "workspaceContains:vite.config.*",
    "workspaceContains:nuxt.config.*",
    "workspaceContains:uno.config.*",
    "workspaceContains:unocss.config.*",
    "workspaceContains:package.json"
  ],
  "devDependencies": {
    "@types/vscode": "^1.62.0",
    "jiti": "^1.13.0",
    "prettier": "^2.6.0",
    "tsup": "^5.12.1",
    "unconfig": "^0.3.2"
  },
  "engines": {
    "vscode": "^1.62.0"
  },
  "icon": "res/logo.png"
}
