{
  "name": "@zhmu/unocss-preset-minipx",
  "version": "0.1.2",
  "description": "Fork from @unocss/preset-mini, change default unit to px.",
  "keywords": [
    "unocss",
    "unocss-preset"
  ],
  "homepage": "https://github.com/chenzhq/unocss/tree/zhmu/preset-px/packages/preset-minipx",
  "license": "MIT",
  "author": "Zhmu <chenzheqi.czq@163.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenzhq/unocss.git",
    "branch": "zhmu/preset-px",
    "directory": "packages/preset-minipx"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./theme": {
      "require": "./dist/theme.cjs",
      "import": "./dist/theme.mjs",
      "types": "./dist/theme.d.ts"
    },
    "./variants": {
      "require": "./dist/variants.cjs",
      "import": "./dist/variants.mjs",
      "types": "./dist/variants.d.ts"
    },
    "./rules": {
      "require": "./dist/rules.cjs",
      "import": "./dist/rules.mjs",
      "types": "./dist/rules.d.ts"
    },
    "./colors": {
      "require": "./dist/colors.cjs",
      "import": "./dist/colors.mjs",
      "types": "./dist/colors.d.ts"
    },
    "./utils": {
      "require": "./dist/utils.cjs",
      "import": "./dist/utils.mjs",
      "types": "./dist/utils.d.ts"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts",
    "*.css"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "@unocss/core": "workspace:*"
  }
}
