{
  "name": "@unocss/monorepo",
  "version": "0.29.6",
  "private": true,
  "packageManager": "pnpm@6.32.3",
  "scripts": {
    "bench": "npm -C bench run bench",
    "build": "rimraf packages/*/dist && esno scripts/copy-files.ts && pnpm -r --filter !playground run build && pnpm -r run build-post",
    "dev": "nr stub",
    "docs": "npm -C docs run dev",
    "docs:build": "npm -C docs run build",
    "lint": "eslint .",
    "lint:fix": "nr lint --fix",
    "play": "npm -C playground run dev",
    "play:build": "pnpm run build && npm -C playground run build",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "size": "esmo scripts/size.ts",
    "stub": "pnpm -r --parallel run stub",
    "test": "vitest",
    "test:update": "vitest -u"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.8",
    "@antfu/ni": "^0.13.2",
    "@iconify-json/carbon": "^1.1.2",
